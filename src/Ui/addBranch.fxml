<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>


<BorderPane fx:controller="Ui.addBranch_Controller"
 xmlns:fx="http://javafx.com/fxml/1">
 
 <center>
 
 <VBox spacing="20" alignment="CENTER">
 	
 
 <Label text="Enter the Branch Name" alignment="CENTER" BorderPane.alignment="center"
 style="-fx-border-width: 5">
 
 	<font>
	<Font name="Times New Roman" size="20"></Font>
	</font>
 	
 
 </Label>
 
 	<HBox spacing="20" alignment="CENTER">
 	<TextField fx:id="branch_name" onKeyReleased="#handleKeyReleased"></TextField>
 	
 	<Button fx:id="submit_branchName" text="Submit" onAction="#onButtonClicked">
 	<padding>
 	<Insets top="15" bottom="15" right="15" left="15" ></Insets>
 	</padding>
 	</Button>



 	</HBox>
 	
 	<Label fx:id="result" text="" alignment="CENTER" 
 	BorderPane.alignment="center" style="-fx-border-width: 5">
 	
 	<font>
	<Font name="Times New Roman" size="20"></Font>
	</font>
	
	</Label>
 
 	
 	
 	
 </VBox>
 
 </center>
 
 
 <bottom>
	<Button fx:id="exit" text="Exit" onAction="#onExit">
	<padding>
	<Insets top="20" right="20" bottom="20" left="20"></Insets>
	</padding>
	</Button>
	</bottom>
 
 
 
	
</BorderPane>

